
.item {
	height: 90rpx;
	background: white;
	display: flex;
	align-items: center;
	border-bottom: 1px solid #F4F4F5;
	font-size: 30rpx;
}
.right-icon {
	position: absolute;
	right: 15px;
}
.right-icon1 {
	position: absolute;
	right: 35px;
	color: gray;
}
.left-icon {
	position: absolute;
	left: 15px;
}
button {
	border: none;
	width: 100%;
	height: 100%;
	text-align: left;
	margin: 0;
	font-size: 30rpx;
	text-indent: 2%;
	background: white;
	line-height: 3;
}
button::after {
	border: none;
	border-radius: 0;
}
.photo {
	width: 35px;
	height: 35px;
}

